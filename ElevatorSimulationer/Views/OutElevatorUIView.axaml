<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:customcontrols="using:ElevatorSimulationer.CustomControls"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="270" d:DesignHeight="920"
             xmlns:prism="http://prismlibrary.com/"
             xmlns:viewmodels="using:ElevatorSimulationer.ViewModels"
             prism:ViewModelLocator.AutoWireViewModel="True"
             x:Class="ElevatorSimulationer.Views.OutElevatorUIView"
             x:DataType="viewmodels:OutElevatorUIViewModel">

  <Border Padding="12">
    <ItemsControl ItemsSource="{Binding OutElevatorFloorModel}">
      <ItemsControl.ItemsPanel>
        <ItemsPanelTemplate>
          <StackPanel ></StackPanel>
        </ItemsPanelTemplate>
      </ItemsControl.ItemsPanel>
      <ItemsControl.DataTemplates>
        <DataTemplate DataType="viewmodels:OutElevatorFloorViewModel">
          <customcontrols:OutElevatorFloorButton Floor="{Binding Floor}"></customcontrols:OutElevatorFloorButton>
        </DataTemplate>
      </ItemsControl.DataTemplates>
    </ItemsControl>
  </Border>

</UserControl>